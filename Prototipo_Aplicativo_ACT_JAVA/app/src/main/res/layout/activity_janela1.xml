<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Janela1"
    tools:layout_editor_absoluteX="16dp"
    tools:layout_editor_absoluteY="16dp">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="140dp"
        android:layout_height="134dp"
        android:layout_marginBottom="38dp"
        android:layout_marginEnd="12dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toTopOf="@+id/pessoal"
        app:layout_constraintEnd_toStartOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/brincar"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/pessoal"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:layout_marginBottom="80dp"
        android:layout_marginEnd="64dp"
        android:layout_marginStart="24dp"
        android:text="pessoas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/comer"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="140dp"
        android:layout_height="134dp"
        android:layout_marginBottom="65dp"
        android:layout_marginEnd="15dp"
        android:layout_marginTop="5dp"
        app:layout_constraintBottom_toTopOf="@+id/comer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/brinquedo"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/comer"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:layout_marginBottom="90dp"
        android:layout_marginEnd="26dp"
        android:text="comidas"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/pessoal"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="140dp"
        android:layout_height="134dp"
        android:layout_marginBottom="30dp"
        android:layout_marginEnd="15dp"
        android:layout_marginTop="63dp"
        app:layout_constraintBottom_toTopOf="@+id/brinquedo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/brinquedo"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:layout_marginBottom="34dp"
        android:layout_marginEnd="62dp"

        android:layout_marginTop="10dp"
        android:text="brinquedos"
        app:layout_constraintBottom_toTopOf="@+id/imageView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/brincar"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="140dp"
        android:layout_height="134dp"
        android:layout_marginBottom="30dp"
        android:layout_marginEnd="25dp"
        android:layout_marginStart="18dp"
        android:layout_marginTop="53dp"
        app:layout_constraintBottom_toTopOf="@+id/brincar"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <Button
        android:id="@+id/brincar"
        android:layout_width="140dp"
        android:layout_height="50dp"
        android:layout_marginBottom="23dp"
        android:layout_marginEnd="66dp"
        android:layout_marginStart="70dp"
        android:text="brincadeiras"
        app:layout_constraintBottom_toTopOf="@+id/imageView3"
        app:layout_constraintEnd_toStartOf="@+id/brinquedo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="16dp"
        android:text="proxima"
        android:onClick="irpagina2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
</android.support.constraint.ConstraintLayout>